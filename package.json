  },
  "devDependencies": {
    "@types/inquirer": "6.0.3",
    "@types/node": "^9.6.49",
    "@types/readline-sync": "^1.4.3",
    "@types/request": "^2.48.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "standard-version": "^5.0.2",
    "tslint": "^5.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.5"
